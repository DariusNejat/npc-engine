# .coveragerc to control coverage.py
[run]
omit =
    */venv/*
    */tests/*
    */docs/*
    */dist/*
    setup.py
    scratch*.py    
    */.tox/*
    */npc_engine/services/tts/flowtron/text/*
    */npc_engine/models/utils/mock.py

[html]
directory=./htmlcov

[report]
fail_under=80